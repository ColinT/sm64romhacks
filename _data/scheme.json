{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "hacks": {
        "type": "array",
        "items": [
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "versions": {
                "type": "array",
                "items": [
                  {
                    "type": "object",
                    "properties": {
                      "versionName": {
                        "type": "string"
                      },
                      "creators": {
                        "type": "array",
                        "items": [
                          {
                            "type": "string"
                          }
                        ]
                      },
                      "starCount": {
                        "type": "string"
                      },
                      "fileName": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "versionName",
                      "creators",
                      "starCount",
                      "fileName"
                    ]
                  }
                ]
              },
              "releaseDate": {
                "type": "string"
              },
              "tag": {
                "type": "string"
              }
            },
            "required": [
              "id",
              "name",
              "versions",
              "releaseDate",
              "tag"
            ]
          }
        ]
      }
    },
    "required": [
      "hacks","starCount"
    ]
  }